{
  "name": "parcel-sample",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npm run serve\" \"npm run server\"",
    "serve": "parcel serve ./src/index.html --port 3001",
    "build": "parcel build ./src/index.html",
    "server": "node ./server.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "jsrsasign": "^10.3.0",
    "open": "^8.2.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "parcel": "^2.0.0-rc.0"
  }
}
